{
  "name": "@flux/legacy-ui",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./src": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./src/*": {
      "import": "./src/*",
      "types": "./src/*"
    },
    "./style.css": "./dist/style.css"
  },
  "files": ["dist"],
  "scripts": {
    "build": "vite build",
    "type-check": "tsc --noEmit",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "vite": "^4.0.0",
    "vue": "^3.3.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0",
    "@vue/test-utils": "^2.4.0",
    "jsdom": "^25.0.0"
  }
}
