{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": false,
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@vitepress/*": [".vitepress/*"]
    },
    "types": ["vitepress/client"]
  },
  "include": [
    ".vitepress/**/*.js"
  ],
  "exclude": [
    "node_modules",
    ".vitepress/cache",
    ".vitepress/dist"
  ]
}
